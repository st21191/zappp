
wait(2)
local code = 4405



game.Workspace.button.Attachment.ProximityPrompt.Triggered:Connect(function()
	print(1)
	for i, v in pairs(game.Players.LocalPlayer.PlayerGui.ScreenGui:GetChildren()) do
		if v:IsA("TextButton") then
			print(2)
			v.BackgroundTransparency = 0
			v.TextTransparency = 0
		elseif v:IsA("TextLabel") then
			v.BackgroundTransparency = 0
			v.TextTransparency = 0
		end
	end
end)
local debounce = false
local limit = 0

script.Parent.b0.MouseButton1Click:Connect(function()
	if limit == 4 then
		debounce = true
	elseif limit < 4 then
		debounce = false
	end
	if debounce == false then
		limit = limit + 1
		script.Parent.Number.Text = script.Parent.Number.Text..'0'
	end
end)


